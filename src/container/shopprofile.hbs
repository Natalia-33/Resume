<main>
{{#with navigation}}
 <div class="card">
    <div class='nav nav tabs'>
      {{#each links}}
        <div class='nav-item'>
          <a href='{{href}}' class='{{#if @last}} disabled {{/if}} nav-link'>{{text}}</a>
        </div>
      {{/each}}
    </div>
    </div>
  {{/with}}
  <br>
  <br>

  <h1>{{header}}</h1>
  <br>

  {{#with purchase}}

  <ul class="nav nav-tabs mb-5">
    {{#each tabs}}
   <li class="nav-link {{#if @first}} active {{/if}}">{{this}}</li>
      {{/each}}
  </ul>


  {{#with actionBlock}}
 <div class="row">
  {{#with input}}
   
  <div class="col-3">
     <input class="form-control" placeholder="{{placeholder}}">
     </div>
     {{/with}}

     {{#each buttons}}
     <div class="col-3">
      {{#if isSuccess}} <div class="btn btn-success">{{text}}</div>{{/if}}
       {{#if isOutline}} <div class="btn btn-outline-success">{{text}}</div>{{/if}}
        {{#if isDanger}} <div class="btn btn-danger">{{text}}</div>{{/if}}
        </div>
     {{/each}}
   </div>
     {{/with}}
     

      <hr>

       {{#each itemList}}
      <div class="row p-1 my-3 p-1">
        <div class="col-3 gy-2">
      <h6>{{number}}</h6>
      <h7><b>{{status}}</b></h7>
      </div>
      
      
      <div class="col-2">
        {{#with amount}}
        <h7>{{title}}</h7>
        <h6>{{value}}</h6>
      {{/with}}
       </div>
       
        <div class="col-4">
      {{#each images}}
       <img src="{{this}}" alt="photos">
      {{/each}}
      </div>
      
      <div class="col-2">
         {{#with button}}
      {{#if isPrimary}} <div class="btn btn-primary btn-sm">{{text}}</div>{{/if}}
      {{/with}}
      </div>
      <hr>
       </div>
     {{/each}}
  {{/with}}


  {{#with userInfo}}
  <h1>{{title}}</h1>
  {{#with profileData}}
  <div class="card">
  <h3>{{title}}</h3>
  {{#with fullName}}
  {{#with surname}}
  <main class="p-5">
    <div class="row g-5">
      <label class="form-label"></label>
      <input class="form-control">
      <div class="form-text">{{title}}</div>

    </div>

  </main>
  {{/with}}
  {{/with}}
  </div>
  {{/with}}
  {{/with}}
  </main>