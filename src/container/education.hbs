<div>
  {{#with main}}
    <main>
      <section>
        {{> title data='My educationss' big=true}}
          <ul class="ps-5">
            {{#each educations}}
            <li>
              {{name}}
              {{#unless isEnd}} (ще навчаюсь) {{/unless}}
            </li>
            {{/each}}
          </ul>
      </section>



      <section>
        {{>title data='My certificates'}}
        <ul class="ps-5">
            {{#each certificates}}
            <li>
              {{name}} ({{id}})
            </li>
            {{/each}}
        </ul>
      </section>
    </main>
  {{/with}}
</div>