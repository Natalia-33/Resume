<div>
  <section class='col-12'>
    {{#with navigation}}
      {{#each links}}
        <a
          href='{{href}}'
          class='btn btn-link col-2'
        >{{text}}</a>
      {{/each}}
      <a
        href='{{button.href}}'
        class='btn btn-primary col-2'
      >{{button.text}}</a>
    {{/with}}
    <hr />
  </section>

  <hr />
  {{#with header}}
    <h1>{{title}}</h1>
    <p class='text-muted'>{{description}}</p>
    {{#with button}}
      <button class='btn btn-primary'>{{text}}</button>
    {{/with}}
  {{/with}}

  <hr />

  {{#with city}}
    <h2>{{title}}</h2>

    <div class='row gx-2 gy-4'>

      {{#each cards}}
        <div class='col-5'>
          <div class='card'>
            <img
              class='card-img-top'
              src='{{picture.src}}'
              alt='{{picture.alt}}'
            />

            <div class='card-body'>
              <h5 class='card-title'>{{title}}</h5>
              <br />
              <p>{{description}}</p>
              <br />

              <a
                href='{{button.href}}'
                class='btn btn-primary'
              >{{button.text}}</a>
            </div>
          </div>
        </div>

      {{/each}}
    </div>
  {{/with}}

  <hr />

  {{#with winner}}
    <h2>{{title}}</h2>
    <div>
      <table class='table'>
        {{#with table}}

          <thead class='table-bordered'>
            <tr>
              {{#each head}}
                <th>{{this}}</th>
              {{/each}}
            </tr>
          </thead>
          <tbody>
            {{#each user}}
              <tr>
                <th>{{place}}</th>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
              </tr>
            {{/each}}
          </tbody>
        {{/with}}
      </table>
    </div>
  {{/with}}

  {{#with gallery}}
    <h2 class='text-center'>{{title}}</h2>
    <div class='row'>
      {{#each cards}}
        <div class='col-3'>
          <div class='card'>
            <div
              class='card-header text-center'
            >{{name}}</div>

            <div class='card-body'>
              <h3 class='card-title text-center'></h3>

              {{#each items}}
                <div
                  class='card-text text-center'
                >{{this}}</div>
              {{/each}}
            </div>
          </div>
        </div>

      {{/each}}
    </div>
  {{/with}}

  {{#with relaxion}}
    <div class='display-3 text-center'>{{title}}</div>
    <div class='row col-10'>
      {{#each cards}}
        <div class='col-6'>
          <div class='card'>
            <div
              class='card-header text-center'
            >{{header}}</div>

            <div class='card-body'>
              <h6
                class='text-center card-title'
              >{{title}}</h6>
              <p class='text-center'>{{description}}</p>
            </div>
          </div>
        </div>
      {{/each}}
    </div>
  {{/with}}

  {{#with rest}}
    <div class='row col-10'>
      {{#each cards}}
        <div class='col-6'>
          <div class='card'>
            <div
              class='card-header text-center'
            >{{header}}</div>
            <div class='card-body'>
              <h3
                class='text-center card-title'
              >{{title}}</h3>
              <p class='text-center'>{{description}}</p>
              <div class='text-center'>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
              </div>
            </div>
            <div
              clas='card-footer text-center'
            >{{footer}}</div>
          </div>
        </div>

      {{/each}}
    </div>
  {{/with}}

  {{#with proposition}}
    <div class='display-2 text-center'>{{title}}</div>
    <div class='row col-10'>
      {{#each card}}
        <div class='col-6'>
          <div class='card'>
            <div
              class='card-header text-center'
            >{{header}}</div>
            <div class='card-body'>
              <h3
                class='text-center card-title'
              >{{title}}</h3>
              <p class='text-center'>{{description}}</p>

            </div>
          </div>
        </div>
      {{/each}}
    </div>
  {{/with}}

  <footer>
    {{#with footer}}
      <h5>{{title}}</h5>

      <div class='row gy-3'>

        <ul class='list-group'>
          {{#each navigation}}
            <li class='list-group-item'>
              <a
                href='{{link}}'
                class='btn btn outline-primary'
              >{{name}}</a>
            </li>
          {{/each}}
        </ul>

        <ul class='list-group'>
          {{#each socialMedia}}
            <li class='list-group-item'>
              <a
                href='{{link}}'
                class='btn btn outline-primary'
              >{{name}}</a>
            </li>
          {{/each}}
        </ul>

        <ul class='list-group'>
          {{#each partners}}
            <li class='list-group-item'>
              <a
                href='{{link}}'
                class='btn btn outline-primary'
              >{{name}}</a>
            </li>
          {{/each}}
        </ul>

      </div>

    {{/with}}
  </footer>
</div>